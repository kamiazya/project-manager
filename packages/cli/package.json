{
  "name": "@project-manager/cli",
  "version": "0.0.0",
  "description": "CLI interface for project-manager",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "bin": {
    "pm": "src/bin/pm-dev.ts"
  },
  "exports": {
    ".": {
      "@project-manager/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
      "pm": "dist/bin/pm.js"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "executableFiles": [
      "dist/bin/pm.js"
    ]
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@project-manager/core": "workspace:*",
    "@project-manager/shared": "workspace:*",
    "chalk": "catalog:",
    "commander": "catalog:",
    "inversify": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
